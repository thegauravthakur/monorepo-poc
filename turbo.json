{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "lint": {
            "outputs": []
        },
        "test": {
            "dependsOn": ["build"],
            "outputs": [],
            "inputs": ["packages/**/*.tsx"]
        },
        "build": {
            "dependsOn": ["^build"]
        },
        "deploy": {
            "dependsOn": ["build", "test", "lint"],
            "outputs": []
        }
    }
}
